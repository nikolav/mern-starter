    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "color": "^4.2.3",
    "compression": "^1.7.4",
    "d3": "^7.6.1",
    "d3-svg-legend": "^2.25.6",
    "d3-tip": "^0.9.1",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "firebase": "^9.9.3",
    "framer-motion": "^7.2.0",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.0",
    "nikolav-q": "^1.2.2",
