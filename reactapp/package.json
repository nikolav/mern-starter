{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "node ./serve ",
    "test": "dotenv -e .env.test -- jest ",
    "test:demo": "dotenv -e .env.test -- jest demo-msw ",
    "dev": "concurrently \"yarn run sass:watch\" \"yarn run watch:css\"  \"vite --host\" ",
    "build": "yarn run sass:build && yarn run build:css && cross-env NODE_ENV=production && vite build",
    "preview": "vite preview",
    "sass:build": "sass ./src/global.scss ./src/styles/global.css ",
    "sass:watch": "sass ./src/global.scss ./src/styles/global.css --watch",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./src/styles/build.css",
    "watch:css": "tailwindcss -i ./src/tailwind.css -o ./src/styles/build.css --watch",
    "lint": "eslint ",
    "docs": "serve -p 8889 docs ",
    "tinker": "node ./tinker.js",
    "format": "prettier --write src "
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
